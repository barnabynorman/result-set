{
  "name": "barnabynorman/result-set",
  "type": "library",
  "description": "Manage collections of items with filters and utilities",
  "license": "MIT",
  "minimum-stability": "dev",
  "require": {
    "php": "^7.0"
  },
    "require-dev": {
      "phpunit/phpunit": "^7.5@dev"
  },
  "autoload": {
    "psr-4": {
      "ResultSet\\": "src/ResultSet/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
      "test": [
          "@phpunit"
      ],
      "phpunit": "phpunit --verbose --coverage-clover=coverage.xml"
  }
}
